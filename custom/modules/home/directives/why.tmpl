	</div>
	<div class="w-100"></div>
	<div class="col-lg-2 col-md-3 col-sm-2" ng-repeat="oneIcon in icons">
		<a href="{{oneIcon.url}}" ng-if="oneIcon.url" alt="{{oneIcon.title}}">
			<img ng-src="{{oneIcon.image}}" />
